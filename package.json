{
  "name": "did-auth",
  "version": "1.0.0",
  "description": "5 Easy Steps To Abandon Redux For The Remarkable React Hooks",
  "main": "index.js",
  "engines": {
    "node": ">=12.16"
  },
  "scripts": {
    "dev": "webpack serve --progress --open --mode development --config webpack.development.config.js",
    "maintain": "npm install & npm update && npm audit fix --force && npm run build-all",
    "setup-did": "node -r @babel/register -r dotenv/config setup-did.js",
    "setup-fauna": "node -r @babel/register -r dotenv/config setup-fauna.js",
    "setup-secrets": "node -r @babel/register setup-secrets.js",
    "functions": "nodemon -r @babel/register -r dotenv/config -w lambda -w effects lambda/functions.js",
    "build-did": "webpack --progress --mode production --config webpack.functions.did.config.js",
    "build-user": "webpack --progress --mode production --config lambda/webpack.functions.user.config.js",
    "build-web": "webpack --progress --mode production --config webpack.production.config.js",
    "build": "npm run build-web",
    "build-all": "npm run build-web && npm run build-did && npm run build-user"
  },
  "author": "Martin Novak",
  "license": "ISC",
  "devDependencies": {
    "@7urtle/lambda": "^1.4.0",
    "@7urtle/logger": "^1.1.4",
    "@7urtle/server": "^1.0.15",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-throw-expressions": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "axios": "^0.21.4",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^1.13.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.1.0",
    "didauth": "^1.2.0",
    "dotenv": "^10.0.0",
    "faunadb": "^4.4.2",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.2.0",
    "ngrok": "^4.2.2",
    "nodemon": "^2.0.15",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "styled-components": "^5.3.3",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "encoding": "^0.1.13"
  }
}
